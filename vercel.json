{
    "builds": [
        {
            "src": "core/wsgi.py",
            "use": "@versel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.13.5"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ]
}


